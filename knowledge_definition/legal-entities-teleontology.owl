<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://example.org/mongolia-kg#"
    xml:base="http://example.org/mongolia-kg"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- ===================== -->
    <!-- Ontology -->
    <!-- ===================== -->

    <owl:Ontology rdf:about="http://example.org/mongolia-kg">
        <rdfs:label>Mongolian Corporate Teleontology (Evolved)</rdfs:label>
        <rdfs:comment>
            An evolved teleontology supporting ownership, control,
            representation, business activity, restructuring,
            and network-level corporate analysis.
        </rdfs:comment>
    </owl:Ontology>

    <!-- ===================== -->
    <!-- Core Classes -->
    <!-- ===================== -->

    <owl:Class rdf:about="#LegalEntity"/>
    <owl:Class rdf:about="#NaturalPerson"/>
    <owl:Class rdf:about="#Organization"/>

    <!-- ===================== -->
    <!-- Role / Event Classes -->
    <!-- ===================== -->

    <owl:Class rdf:about="#CorporateRole"/>
    <owl:Class rdf:about="#AuthorizedRepresentation">
        <rdfs:subClassOf rdf:resource="#CorporateRole"/>
    </owl:Class>

    <owl:Class rdf:about="#BeneficialOwnership">
        <rdfs:subClassOf rdf:resource="#CorporateRole"/>
    </owl:Class>

    <owl:Class rdf:about="#BusinessActivity"/>
    <owl:Class rdf:about="#RestructuringEvent"/>

    <!-- ===================== -->
    <!-- Object Properties -->
    <!-- ===================== -->

    <!-- Representation -->
    <owl:ObjectProperty rdf:about="#hasAuthorizedRepresentative">
        <rdfs:domain rdf:resource="#LegalEntity"/>
        <rdfs:range rdf:resource="#AuthorizedRepresentation"/>
        <owl:inverseOf rdf:resource="#representsEntity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#representsEntity">
        <rdfs:domain rdf:resource="#AuthorizedRepresentation"/>
        <rdfs:range rdf:resource="#LegalEntity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#representedBy">
        <rdfs:domain rdf:resource="#CorporateRole"/>
        <rdfs:range rdf:resource="#NaturalPerson"/>
        <owl:inverseOf rdf:resource="#holdsRole"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#holdsRole">
        <rdfs:domain rdf:resource="#NaturalPerson"/>
        <rdfs:range rdf:resource="#CorporateRole"/>
    </owl:ObjectProperty>

    <!-- Beneficial Ownership -->
    <owl:ObjectProperty rdf:about="#hasUltimateBeneficialOwner">
        <rdfs:domain rdf:resource="#LegalEntity"/>
        <rdfs:range rdf:resource="#BeneficialOwnership"/>
    </owl:ObjectProperty>

    <!-- Business Activity -->
    <owl:ObjectProperty rdf:about="#hasBusinessActivity">
        <rdfs:domain rdf:resource="#LegalEntity"/>
        <rdfs:range rdf:resource="#BusinessActivity"/>
    </owl:ObjectProperty>

    <!-- Restructuring -->
    <owl:ObjectProperty rdf:about="#hasRestructuringEvent">
        <rdfs:domain rdf:resource="#LegalEntity"/>
        <rdfs:range rdf:resource="#RestructuringEvent"/>
    </owl:ObjectProperty>

    <!-- ===================== -->
    <!-- Datatype Properties -->
    <!-- ===================== -->

    <owl:DatatypeProperty rdf:about="#registrationNumber">
        <rdfs:domain rdf:resource="#LegalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#registeredName">
        <rdfs:domain rdf:resource="#LegalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#roleStartDate">
        <rdfs:domain rdf:resource="#CorporateRole"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#positionTitle">
        <rdfs:domain rdf:resource="#AuthorizedRepresentation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#uboClassification">
        <rdfs:domain rdf:resource="#BeneficialOwnership"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#activityName">
        <rdfs:domain rdf:resource="#BusinessActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#activityStatus">
        <rdfs:domain rdf:resource="#BusinessActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#restructuringType">
        <rdfs:domain rdf:resource="#RestructuringEvent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#previousName">
        <rdfs:domain rdf:resource="#RestructuringEvent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#eventDate">
        <rdfs:domain rdf:resource="#RestructuringEvent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    </owl:DatatypeProperty>

    <!-- ===================== -->
    <!-- Constraints -->
    <!-- ===================== -->

    <!-- Every AuthorizedRepresentation must have exactly one person -->
    <owl:Class rdf:about="#AuthorizedRepresentation">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#representedBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#NaturalPerson"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>


    
    <!-- ===================== -->
    <!-- Annotation Properties -->
    <!-- ===================== -->

    <owl:AnnotationProperty rdf:about="#mnLabel"/>
    <owl:AnnotationProperty rdf:about="#dataSource"/>
    <owl:AnnotationProperty rdf:about="#originalField"/>

    <!-- ===================== -->
    <!-- Legal Entity Typology -->
    <!-- ===================== -->

    <owl:Class rdf:about="#Company">
        <rdfs:subClassOf rdf:resource="#LegalEntity"/>
    </owl:Class>

    <owl:Class rdf:about="#LimitedLiabilityCompany">
        <rdfs:subClassOf rdf:resource="#Company"/>
        <rdfs:label>Limited Liability Company</rdfs:label>
        <mnLabel>Хязгаарлагдмал хариуцлагатай компани</mnLabel>
    </owl:Class>

    <owl:Class rdf:about="#ForProfitEntity">
        <rdfs:subClassOf rdf:resource="#LegalEntity"/>
        <mnLabel>Ашгийн төлөө</mnLabel>
    </owl:Class>

    <!-- ===================== -->
    <!-- Legal Entity: Ташир агро фийд -->
    <!-- ===================== -->

    <owl:NamedIndividual rdf:about="#LE_3199665">
        <rdf:type rdf:resource="#LimitedLiabilityCompany"/>
        <rdf:type rdf:resource="#ForProfitEntity"/>
        <registrationNumber>3199665</registrationNumber>
        <registeredName>Ташир агро фийд</registeredName>
        <registrationDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-03-05</registrationDate>
        <dataSource>State Registration of Legal Entities</dataSource>
    </owl:NamedIndividual>

    <!-- ===================== -->
    <!-- Person: Даваасүрэн Эрдэнэтуяа -->
    <!-- ===================== -->

    <owl:NamedIndividual rdf:about="#Person_Erdenetuya_Davaasuren">
        <rdf:type rdf:resource="#NaturalPerson"/>
        <rdfs:label>Erdenetuya Davaasuren</rdfs:label>
        <mnLabel>Даваасүрэн Эрдэнэтуяа</mnLabel>
    </owl:NamedIndividual>

    <!-- Authorized Representation -->

    <owl:NamedIndividual rdf:about="#AuthRep_3199665_01">
        <rdf:type rdf:resource="#AuthorizedRepresentation"/>
        <representedBy rdf:resource="#Person_Erdenetuya_Davaasuren"/>
        <representsEntity rdf:resource="#LE_3199665"/>
        <positionTitle>Гүйцэтгэх захирал</positionTitle>
        <roleStartDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-03-05</roleStartDate>
    </owl:NamedIndividual>

    <!-- ===================== -->
    <!-- Business Activities -->
    <!-- ===================== -->

    <owl:NamedIndividual rdf:about="#BA_3199665_01">
        <rdf:type rdf:resource="#BusinessActivity"/>
        <activityName>Газар тариалан</activityName>
        <activityStatus>Active</activityStatus>
        <registrationDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-03-05</registrationDate>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#BA_3199665_02">
        <rdf:type rdf:resource="#BusinessActivity"/>
        <activityName>Мал аж ахуй</activityName>
        <activityStatus>Active</activityStatus>
        <registrationDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-03-05</registrationDate>
    </owl:NamedIndividual>

    <hasBusinessActivity rdf:resource="#BA_3199665_01"/>
    <hasBusinessActivity rdf:resource="#BA_3199665_02"/>

    <!-- ===================== -->
    <!-- Legal Entity: РБМК -->
    <!-- ===================== -->

    <owl:NamedIndividual rdf:about="#LE_6673252">
        <rdf:type rdf:resource="#LimitedLiabilityCompany"/>
        <rdf:type rdf:resource="#ForProfitEntity"/>
        <registrationNumber>6673252</registrationNumber>
        <registeredName>РБМК</registeredName>
        <registrationDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-03-05</registrationDate>
    </owl:NamedIndividual>

    <!-- ===================== -->
    <!-- Person: Цэдэнбалжир Идэрбүргэд -->
    <!-- ===================== -->

    <owl:NamedIndividual rdf:about="#Person_Iderburged_Tsedenbaljir">
        <rdf:type rdf:resource="#NaturalPerson"/>
        <rdfs:label>Iderburged Tsedenbaljir</rdfs:label>
        <mnLabel>Цэдэнбалжир Идэрбүргэд</mnLabel>
    </owl:NamedIndividual>

    <!-- Authorized Role -->

    <owl:NamedIndividual rdf:about="#AuthRep_6673252_01">
        <rdf:type rdf:resource="#AuthorizedRepresentation"/>
        <representedBy rdf:resource="#Person_Iderburged_Tsedenbaljir"/>
        <representsEntity rdf:resource="#LE_6673252"/>
        <positionTitle>Гүйцэтгэх захирал</positionTitle>
    </owl:NamedIndividual>

    <!-- Beneficial Ownership -->

    <owl:NamedIndividual rdf:about="#UBO_6673252_01">
        <rdf:type rdf:resource="#BeneficialOwnership"/>
        <representedBy rdf:resource="#Person_Iderburged_Tsedenbaljir"/>
        <uboClassification>Иргэн</uboClassification>
    </owl:NamedIndividual>

    <hasUltimateBeneficialOwner rdf:resource="#UBO_6673252_01"/>

</rdf:RDF>


